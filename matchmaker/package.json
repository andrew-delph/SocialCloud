{
  "name": "matchmaker",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "prod": "node ./dist/ready-worker.js",
    "dev": "nodemon --delay 1000ms --ignore dist --watch ./src  --ext ts --exec 'npm run k8-restart'",
    "dev2": "tsc-watch --onSuccess \"npm run k8-restart\" --onCompilationStarted \"echo 'watching matchmaker'\"",
    "docker": "bazel run image_push",
    "k8-restart": "npm run docker && kubectl rollout restart deployment/matchmaker && kubectl rollout status deployment/matchmaker --watch=true --timeout=60s && date"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
