    "@types/uuid": "^9.0.0",
    "@types/amqplib": "^0.10.1",
    "@types/lodash.throttle": "^4.1.7",
    "@types/node": "^18.13.0",
    "tsc-watch": "^6.0.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "cluster": "^0.7.7",
    "redlock": "^5.0.0-beta.2",
    "ioredis": "5.2.5",
    "uuid": "^9.0.0",
    "amqplib": "^0.10.3",
    "lodash": "^4.17.21",
    "lodash.throttle": "^4.1.1",
    "neo4j-grpc-common": "link:../neo4j/neo4j-grpc-common",
    "common": "link:../common"
  }
}
