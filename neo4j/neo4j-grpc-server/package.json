  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon --delay 1000ms --ignore dist --watch ./src  --ext ts --exec 'npm run k8-restart'",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "prod": "node ./dist/index.js",
    "docker": "docker build -f Dockerfile  -t andrewdelph/video-call-neo4j-grpc-server:latest ../../ && docker push andrewdelph/video-call-neo4j-grpc-server:latest",
