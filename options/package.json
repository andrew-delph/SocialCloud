{
  "name": "options",
  "version": "1.0.0",
  "description": "",
  "main": "dist/options.js",
  "scripts": {
    "prod": "node ./dist/options.js",
    "build": "tsc",
    "build:watch": "nodemon --watch ./src -e ts --exec 'npm run build'",
    "dev": "nodemon --delay 1000ms --ignore dist --watch ./src  --ext ts --exec 'npm run k8-restart'",
    "docker": "bazel run image_push",
    "k8-restart": "npm run docker && kn service update options --image=andrewdelph/video-call-options:latest --wait && date"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "ioredis": "5.2.5",
    "neo4j-driver": "^5.5.0",
    "omit-deep-lodash": "^1.1.7",
    "firebase-admin": "^11.4.1",
    "common-messaging": "link:../common-messaging",
    "common": "link:../common"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "nodemon": "^2.0.20",
    "typescript": "^4.4.4"
  },
  "author": "",
  "license": "ISC"
}
