{
  "name": "options",
  "version": "1.0.0",
  "description": "",
  "main": "dist/options.js",
  "scripts": {
    "prod": "node ./dist/options.js",
    "build": "tsc",
    "build:watch": "nodemon -e ts --exec 'npm run build'",
    "dev": "ibazel -run_command_after_success='bash -c \"cd options && npm run k8-restart\"' build :ts",
    "docker": "bazel run image_push",
    "k8-restart": "bazel run :image_push && kn service update options --image=ghcr.io/andrew-delph/video-call-options:latest --wait && date || echo -en '\\0007'"
  },
  "dependencies": {
    "common": "link:../common",
    "common-messaging": "link:../common-messaging",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "firebase-admin": "^11.4.1",
    "ioredis": "5.2.5",
    "moment": "^2.29.4",
    "neo4j-driver": "^5.5.0",
    "omit-deep-lodash": "^1.1.7"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "nodemon": "^2.0.20",
    "typescript": "^4.4.4"
  },
  "author": "",
  "license": "ISC"
}
