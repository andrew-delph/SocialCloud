{
  "name": "react-video-call-websocket",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --delay 1000ms --ignore dist --watch ./src  --ext ts --exec 'npm run k8-restart'",
    "build": "tsc",
    "prod": "node ./dist/socketio_server.js",
    "docker": "bazel run image_push",
    "k8-restart": "npm run docker && kn service update websocket --image=andrewdelph/video-call-websocket:latest --wait && date"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@socket.io/redis-adapter": "8.0.0",
    "@socket.io/sticky": "^1.0.2",
    "amqplib": "^0.10.3",
    "cluster": "^0.7.7",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "firebase-admin": "^11.4.1",
    "ioredis": "5.2.5",
    "lodash": "^4.17.21",
    "neo4j-grpc-common": "^1.0.0",
    "react-video-call-common": "^1.0.0",
    "socket.io": "4.6.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.1",
    "@types/express": "^4.17.15",
    "@types/lodash": "^4.14.191",
    "@types/uuid": "^9.0.0",
    "tsc-watch": "^6.0.0",
    "typescript": "^4.9.4"
  },
  "workspaces": {
    "packages": [
      "../common",
      "../neo4j/neo4j-grpc-common"
    ]
  }
}
