{
  "name": "react-video-call-websocket",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --delay 1000ms --ignore dist --ext ts --exec 'npm run k8-restart'",
    "dev2": "tsc-watch --onSuccess \"npm run k8-restart\" --onCompilationStarted \"echo 'watching websocket'\"",
    "build": "tsc",
    "prod": "node ./dist/index.js",
    "docker": "docker build -f Dockerfile  -t andrewdelph/video-call-websocket:latest ../ && docker push andrewdelph/video-call-websocket:latest",
    "k8-restart": "npm run docker && kubectl rollout restart deployment/websocket && kubectl rollout status deployment/websocket --watch=true --timeout=60s && kubectl logs --selector=app=websocket -f"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "socket.io": "4.5.4",
    "@socket.io/redis-adapter": "8.0.0",
    "@socket.io/sticky": "^1.0.2",
    "amqplib": "^0.10.3",
    "cluster": "^0.7.7",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "firebase-admin": "^11.4.1",
    "ioredis": "5.2.5",
    "lodash": "^4.17.21",
    "lodash.throttle": "^4.1.1",
    "neo4j-grpc-common": "file:../neo4j/neo4j-grpc-common",
    "react-video-call-common": "^1.0.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.1",
    "@types/express": "^4.17.15",
    "@types/lodash.throttle": "^4.1.7",
    "@types/uuid": "^9.0.0",
    "tsc-watch": "^6.0.0",
    "typescript": "^4.9.4"
  },
  "workspaces": {
    "packages": [
      "../common"
    ]
  }
}
